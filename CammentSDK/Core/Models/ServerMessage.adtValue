%type name="Camment"
%type name="Invitation"
%type name="UserJoinedMessage"
%type name="CammentDeletedMessage"

ServerMessage includes(RMBuilder) {
  invitation {
    Invitation *invitation
  }
  camment {
    Camment *camment
  }
  userJoined {
    UserJoinedMessage *userJoinedMessage
  }
  cammentDeleted {
    CammentDeletedMessage *cammentDeletedMessage
  }
}
